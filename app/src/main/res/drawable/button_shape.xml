<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape android:shape="rectangle">

            <corners
                android:topLeftRadius="72dp"
                android:topRightRadius="72dp"
                android:bottomLeftRadius="72dp"
                android:bottomRightRadius="72dp"
                />
            <solid
                android:color="@color/colorWhite"
                />
            <padding
                android:left="2dp"
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp"
                />
            <size
                android:width="72dp"
                android:height="72dp"
                />
            <stroke
                android:width="1.5dp"
                android:color="@color/colorWhite"
                />

        </shape>
    </item>
    <item >
        <shape android:shape="rectangle">

            <corners
                android:topLeftRadius="72dp"
                android:topRightRadius="72dp"
                android:bottomLeftRadius="72dp"
                android:bottomRightRadius="72dp"
                />
            <solid
                android:color="@color/colorPrimaryDark"
                />
            <padding
                android:left="3.5dp"
                android:top="3.5dp"
                android:right="3.5dp"
                android:bottom="3.5dp"
                />
            <size
                android:width="72dp"
                android:height="72dp"
                />
            <stroke
                android:width="3dp"
                android:color="@color/colorDarkBlue"
                />

        </shape>
    </item>

</layer-list>